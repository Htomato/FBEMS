<!--pages/PM/contacts/add/add.wxml-->
<view class="page bg-fff">
  <view class="weui-form">
    <view class="weui-form__text-area">
      <h2 class="weui-form__title">专家详情</h2>
    </view>
    <view class="weui-cells__group weui-cells__group_form">
      <view class="weui-cells weui-cells_form">
        <view class="weui-cell">
          <view class="weui-cell__hd">
            <label class="weui-label">姓名:</label>
          </view>
          <view class="weui-cell-bd">
            <view>{{eqExpert.eqexpertName}}</view>
          </view>
        </view>

        <view class="weui-cell weui-cell_active">
          <view class="weui-cell__hd">
            <label class="weui-label">单位:</label>
          </view>
          <view class="weui-cell__bd">
            <view>{{eqExpert.eqexpertCompany}}</view>
          </view>
        </view>
        <view class="weui-cell weui-cell_active">
          <view class="weui-cell__hd">
            <label class="weui-label">职称:</label>
          </view>
          <view class="weui-cell-bd">
            <view>{{eqExpert.eqexpertQua}}</view>
          </view>
        </view>

        <view class="weui-cell weui-cell_active">
          <view class="weui-cell__hd">
            <label class="weui-label">领域:</label>
          </view>
          <view class="weui-cell__bd">
            <view>{{eqExpert.eqexpertDomain}}</view>
          </view>

        </view>


        <view class="weui-cell weui-cell_active">
          <view class="weui-cell__hd">
            <label class="weui-label">详情:</label>
          </view>
          <view class="weui-cell__bd">
            <textarea class="weui-textarea" rows="3" value="{{eqExpert.eqexpertDetail}}" disabled></textarea>
          </view>
        </view>
        <view class="weui-cell weui-cell_active">
          <view class="weui-cell__hd">
            <label class="weui-label">资质:</label>
          </view>
          <view class="weui-cell__bd">
            <image src="{{eqExpert.eqexpertQuaimage}}" class="avatar"></image>
          </view>
        </view>

      </view>
    </view>
  </view>
</view>